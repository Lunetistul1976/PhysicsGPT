{
  "version": 2,
  "builds": [
    {
      "src": "src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/google-docs/auth/url",
      "dest": "src/app.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://physics-gpt-good.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/google-docs/auth/callback",
      "dest": "src/app.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://physics-gpt-good.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/google-docs/auth/set-credentials",
      "dest": "src/app.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://physics-gpt-good.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/google-docs/create",
      "dest": "src/app.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://physics-gpt-good.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/google-docs/(.*)",
      "dest": "src/app.ts",
      "headers": {
        "Access-Control-Allow-Origin": "https://physics-gpt-good.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "src/app.ts"
    }
  ],
  "env": {
    "GOOGLE_CLIENT_ID": "@google_client_id",
    "GOOGLE_CLIENT_SECRET": "@google_client_secret",
    "GOOGLE_REDIRECT_URI": "@google_redirect_uri"
  }
} 